defines =
include_dirs =
cflags = -fPIC -pthread
cflags_cc =
target_out_dir = obj
target_output_name = hello

build obj/hello.hello.o: cxx ../hello.cc

build hello: link obj/hello.hello.o ./libhello_shared.so obj/libhello_static.a
  ldflags = -Wl,-rpath=\$$ORIGIN/ -Wl,-rpath-link=
  libs =
  frameworks =
  output_extension = 
  output_dir = .
